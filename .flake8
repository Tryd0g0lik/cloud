[flake8]
extend-ignore = E111, E302, E402, E501, E303
exclude =
    .git,
    **/__pycache__,
    conf*,
    old,
    build,
    dist
	*/frontend,
    */venv/*,
    */.github,
    */.*,
    */main.py,
    */.editorconfig
    */archive
    */Python
per-file-ignores =
    # imported but unused
    __init__.py: F401